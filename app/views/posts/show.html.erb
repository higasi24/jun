<div class="month-wrapper">
  <div class="month-header">
    <div class="month-header-sub">
      <span class="month-archive">― ARCHIVES ―</span>
      <h1 class="month-month">2019年 5月</h1>
    </div>
  </div>
  <div>
    <div>
      <div>
        <% @contents.each do |yyyymm, count| %>
        <h1>
          <%= link_to ymconv(yyyymm, count.to_s), post_content_path(@post, yyyymm) %></li>
        </h1>
        <% end %>
      </div>
    </div>
  </div>
</div>